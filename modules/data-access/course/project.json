    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "modules/data-access/course/**/*.ts",
          "modules/data-access/course/**/*.html"
        ]
      }
